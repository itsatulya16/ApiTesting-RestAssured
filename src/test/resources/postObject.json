{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id", "name", "createdAt", "data"],
  "properties": {
    "id": { "type": "string" },
    "name": { "type": "string" },
    "createdAt": { "type": "string" },
    "data": {
      "type": "object",
      "required": ["year", "price", "harddisksize", "cpumodel"],
      "properties": {
        "year": { "type": "string" },
        "price": { "type": "string" },
        "harddisksize": { "type": "string" },
        "cpumodel": { "type": "string" }
      }
    }
  }
}